{
  "extends": [
    "stylelint-config-standard",
    "./node_modules/prettier-stylelint/config.js"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extends", "tailwind"]
      }
    ],
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s"]
  }
}
